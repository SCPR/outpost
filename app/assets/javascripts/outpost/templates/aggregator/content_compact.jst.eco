<div class="content-compact">
  <a href="<%= @content.link_path %>"><%= @content.short_headline %></a>
  
  <div style="font-size:11px;">
    <% if @content.published_at?: %>
      <%= moment(@content.published_at).format("YYYY-MM-DD, h:mma") %>
    <% else: %>
      <span class="text-error"><strong>UNPUBLISHED</strong></span>
    <% end %>
  
    (<a href="<%=@content.edit_path%>"><%= @content.obj_key%></a>)
  </div>
</div>
